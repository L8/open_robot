package com.openrobot.touchrobot;

import android.app.Activity;
import android.os.Bundle;
import android.widget.Button;

public class ActivityChooser extends Activity {
    /** Called when the activity is first created. */
    @Override
    public void onCreate(Bundle savedInstanceState) {
        super.onCreate(savedInstanceState);
        setContentView(R.layout.main);
        
        Button onRobotButton = (Button) this.findViewById(R.id.on_robot_activity_button);
        Button controlRobotButton = (Button) this.findViewById(R.id.control_robot_activity_button);
        
    }
}